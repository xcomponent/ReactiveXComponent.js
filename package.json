{
  "name": "reactivexcomponent.js",
  "version": "3.1.6",
  "description": "Javascript reactive client API for XComponent",
  "directories": {
    "test": "test",
    "lib": "./lib"
  },
  "scripts": {
    "dev": "webpack-cli -d --watch",
    "build": "webpack-cli -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "test": "jest --coverage "
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*(Test|Spec))\\.tsx?$",
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/**/*.d.ts"
    ],
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "unmockedModulePathPatterns": [
      "./node_modules/jasmine-reporters"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcomponent/ReactiveXComponent.js.git"
  },
  "keywords": [
    "xcomponent",
    "api",
    "javascript",
    "websocket"
  ],
  "author": "xcomponent",
  "license": "Apache-2.0",
  "main": "./lib/xcomponentapi",
  "types": "./lib/types/index.d.ts",
  "files": [
    "LICENSE",
    "Readme.md",
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/xcomponent/ReactiveXComponent.js/issues"
  },
  "homepage": "https://github.com/xcomponent/ReactiveXComponent.js#readme",
  "dependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.0.0",
    "@types/pako": "^1.0.0",
    "@types/rx": "^4.1.1",
    "@types/xml2js": "0.4.3",
    "@types/xmldom": "^0.1.28",
    "atob": "^2.0.3",
    "log4ts": "^0.4.2",
    "pako": "^1.0.3",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6",
    "uuid": "^3.0.1",
    "websocket": "^1.0.25",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.14",
    "compression": "^1.6.2",
    "jasmine-reporters": "^2.2.0",
    "jest": "^23.0.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "mock-socket": "^8.0.0",
    "source-map-loader": "^0.2.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "^4.0.0",
    "tslint": "^5.0.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.1.4",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
